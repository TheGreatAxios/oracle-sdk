{
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "checkJs": false,
    "allowJs": false,
    "declaration": true,
    "declarationMap": true,
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": true,                /* Report errors on unused locals. */
    "noUnusedParameters": true             /* Report errors on unused parameters. */
  }
}
